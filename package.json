  },
  "devDependencies": {
    "poi": "^8.0.0-rc.3",
    "eslint-config-vue-app": "^1.2.1",
    "xo": "^0.18.1"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "vue-github-badge": "^1.0.0"
  }
