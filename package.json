{
  "private": true,
  "name": "vuemarket",
  "description": "Fantastic Vue Components and Libraries.",
  "version": "0.0.1",
  "main": "src/index.js",
  "repository": {
    "url": "sinchang/vuemarket",
    "type": "git"
  },
  "scripts": {
